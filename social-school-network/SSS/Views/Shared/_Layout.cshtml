            }, 60000);
        }

        function GetAllUsers() {
            $.ajax({
                type: "GET",
                url: "NewsFeed/GetAllUsersByUserId",
                dataType: "json",
                data: null,
                success: function (result, status, xhr) {
                    $("#activememberslist").html("");
                    $("#activememberslist").append(result);
                },
                error: function (xhr, status, error) {

                }
            });

        }

        function GetUserImageAndName() {
            $.ajax({
                type: "GET",
                url: "NewsFeed/GetUserImageAndNameByUserId",
                dataType: "json",
                data: null,
                success: function (result, status, xhr) {
                    var a = result;
                    if (result.ImagePath != null && result.ImagePath != "") {
                        var imagepath = "/Images/User-Profile-Images/" + result.ImagePath;
                        $("#userimagepath").attr("src", imagepath);
                    }
                    $(".u-name-display").text(result.FirstName + " " + result.LastName);

