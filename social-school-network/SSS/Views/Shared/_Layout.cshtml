    <script type="text/javascript">

        $(document).ready(function () {


            $('#checkMeOut').change(function () {
                if ($('#checkMeOut').prop('checked')) {
                    $("body").attr('data-theme', 'dark');
                }
                else {
                    $("body").removeAttr('data-theme');
                }
            });

            $(".uploadWallpaper").click(function () {

                $("#WallpaperUpload").click();

            });

            $("#userimagepath").click(function () {

                $("#ProfileImageUpload").click();

            });

           

            $('#WallpaperUpload').on('change', function () {

                var files = $("#WallpaperUpload").get(0).files;


                if (files != null) {
                    WallPaperUploadAjax(files);
                }

            });

            $('#ProfileImageUpload').on('change', function () {

                var files = $("#ProfileImageUpload").get(0).files;


                if (files != null) {
                    ProfileImageUploadAjax(files);
                }

            });

          
        });


        function OpenChatBox(elem) {
            
            var imgPath = $(elem).attr('image-path');
            var userName = $(elem).attr('user-name');            
            $("#chatBoxUserName").text(userName);
            $("#chatBoxuserImage").attr("src",imgPath);
        }

        function WallPaperUploadAjax(files) {
            var fileData = new FormData();
            for (var i = 0; i < files.length; i++) {
                fileData.append("WallpaperUpload", files[i]);
            }

            $.ajax({
                type: "POST",
                url: "NewsFeed/UploadWallpaper",
                dataType: "json",
                contentType: false, // Not to set any content header
                processData: false, // Not to process data
                data: fileData,
                success: function (result, status, xhr) {
                    window.location.reload();
                },
                error: function (xhr, status, error) {

                }


            });
        }

        function ProfileImageUploadAjax(files) {
            var fileData = new FormData();
            for (var i = 0; i < files.length; i++) {
                fileData.append("WallpaperUpload", files[i]);
            }

            $.ajax({
                type: "POST",
                url: "NewsFeed/UploadUserProfileImage",
                dataType: "json",
                contentType: false, // Not to set any content header
                processData: false, // Not to process data
                data: fileData,
                success: function (result, status, xhr) {
                    window.location.reload();
                },
                error: function (xhr, status, error) {

                }


            });
        }


    </script>
